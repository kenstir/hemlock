<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- the label shown in the android account manager -->
    <string name="ou_account_label">Example Library</string>
    <!-- title shown on sign in activity -->
    <string name="ou_account_sign_in_message"></string>
    <!-- label above choose library spinner, leave empty for library-specific app -->
    <string name="ou_choose_library_label">Choose your library system:</string>
    <!-- label above sign_in field on generic Hemlock app -->
    <string name="ou_sign_in_label">Login to your account:</string>
    <!-- android account manager internal type.
         Each evergreen-derived app must have its own unique ou_account_type. -->
    <string name="ou_account_type">org.evergreen_ils.example</string>
    <!-- app name used to launch app -->
    <string name="ou_app_label">Evergreen</string>
    <!-- library name displayed in the app bar -->
    <string name="ou_library_name">EXAMPLE</string>
    <!-- subtitle displayed in the app bar -->
    <string name="ou_activity_subtitle">%1$s - %2$s</string>
    <!-- OPAC URL -->
    <string name="ou_library_url">https://demo.evergreencatalog.com</string>
    <!-- URL of feedback form -->
    <string name="ou_feedback_url"></string>
    <!-- name of class that supplies menu handler for main menu extensions -->
    <string name="ou_menu_provider"></string>
    <!-- name of class that implements AppBehavior -->
    <string name="ou_behavior_provider"></string>
    <!-- email address to which debug logs may be sent -->
    <string name="ou_developer_email">kenstir.apps@gmail.com</string>
    <!-- sort keyword to use in the open-ils.search.biblio.multiclass.query request, e.g. poprel
         if empty the sort() clause is omitted -->
    <string name="ou_sort_by"></string>
    <!-- if non-empty, the Barcode format to display for Show Card -->
    <string name="ou_barcode_format"></string>
    <!-- if non-empty, the text of link to add to Item Details, e.g. "Recommended Reads" -->
    <string name="ou_details_link_text"></string>
    <!-- if non-empty, the URL query to add to Item Details -->
    <string name="ou_details_link_query"></string>
    <!-- if non-empty, the URL fragment to add to Item Details /eg/opac/record/6276882#awards -->
    <string name="ou_details_link_fragment"></string>
    <!-- if true, organize library spinners hierarchically -->
    <bool name="ou_hierarchical_org_tree">true</bool>
    <!-- if true, group copy information by system and branch, instead of by branch -->
    <bool name="ou_group_copy_info_by_system">false</bool>
    <!-- if true, copy information links to the library info page -->
    <bool name="ou_enable_copy_info_web_links">false</bool>
    <!-- if true, enable phone notification controls on Place Hold screen -->
    <bool name="ou_enable_phone_notification">false</bool>
    <!-- if true, allow patron to see their position in hold queue -->
    <bool name="ou_enable_hold_queue_position">true</bool>
    <!-- if true, show "expires DATE" for holds that are Available -->
    <bool name="ou_enable_hold_shelf_expiration">true</bool>
    <!-- if true, we query an item for parts to allow a part hold -->
    <bool name="ou_enable_part_holds">false</bool>
    <!-- if true, and the item has parts, try title_hold.is_possible and if so, allow title hold.
         if false, require a part hold for an item with parts. -->
    <bool name="ou_enable_title_hold_on_item_with_parts">false</bool>
    <!-- if true, enable messages link in app bar -->
    <bool name="ou_enable_messages">false</bool>
    <!-- if true, enable Pay Fines button based on settings -->
    <bool name="ou_enable_pay_fines">true</bool>
    <!-- if true, display label host name in Item Details view for online items -->
    <bool name="ou_show_online_access_hostname">true</bool>
    <!-- if true, clicking Online Access always shows a popup, even if there is only 1 link -->
    <bool name="ou_always_popup_online_links">false</bool>
    <!-- if true, we must load MARCXML records to load online locations -->
    <bool name="ou_need_marc_record">false</bool>
    <!-- if true, display the hours of operation on the Library Info screen -->
    <bool name="ou_enable_hours_of_operation">true</bool>
    <!-- if true, Firebase Analytics is initialized -->
    <bool name="ou_enable_analytics">false</bool>
    <!-- if true, use an extra container() query to weed deleted items from bookbags -->
    <bool name="ou_extra_bookbag_query">true</bool>
    <!-- max number of results to return from Search -->
    <integer name="ou_search_limit">200</integer>
</resources>
